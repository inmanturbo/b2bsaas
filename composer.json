{
    "name": "inmanturbo/b2bsaas",
    "type": "library",
    "license": "MIT",
    "description": "B2B SaaS boilerplate for Laravel",
    "keywords": [
        "laravel",
        "b2b",
        "saas",
        "boilerplate"
    ],
    "bin": "bin/b2bsaas",
    "autoload": {
        "psr-4": {
            "Inmanturbo\\B2bSaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "authors": [
        {
            "name": "inmanturbo"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "@dev",
        "laravel/sanctum": "^3.3",
        "laravel/jetstream": "^4.2",
        "livewire/volt": "^1.6",
        "laravel/folio": "^1.1",
        "spatie/laravel-navigation": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inmanturbo\\B2bSaas\\B2bSaasServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.33",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "larastan/larastan": "^2.8",
        "orchestra/testbench": "^8.21"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
