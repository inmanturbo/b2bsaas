{
    "name": "inmanturbo/b2bsaas",
    "type": "library",
    "license": "MIT",
    "description": "B2B SaaS boilerplate for Laravel",
    "keywords": [
        "laravel",
        "b2b",
        "saas",
        "boilerplate"
    ],
    "bin": "bin/b2bsaas",
    "autoload": {
        "psr-4": {
            "Inmanturbo\\B2bSaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "authors": [
        {
            "name": "inmanturbo"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/inmanturbo/laravel-data"
        },
        {
            "type": "vcs",
            "url": "https://github.com/inmanturbo/php-structure-discoverer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "@dev",
        "laravel/sanctum": "^4.0",
        "laravel/jetstream": "^5.0@dev",
        "livewire/volt": "^1.6",
        "laravel/folio": "^1.1",
        "spatie/laravel-navigation": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inmanturbo\\B2bSaas\\B2bSaasServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
